{
  "manifest_version": 3,
  "name": "Web Request Interceptor",
  "version": "1.0",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "debugger",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": ["<all_urls>"]
  }
}
